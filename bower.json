{
  "name": "angular-ical",
  "version": "0.0.0",
  "dependencies": {
    "angular": ">1.2.0",
    "ical": "git@github.com:mozilla-comm/ical.js.git"
  },
  "devDependencies": {
    "angular-mocks": ">1.2.0",
    "es5-shim": "~4.1.1"
  },
  "main": [
    "dist/js/angular-ical.js"
  ],
  "appPath": "src",
  "authors": [
    "steve mao <maochenyan@gmail.com>"
  ],
  "description": "angular-ical is an Angular factory wrapper for ical.js (https://github.com/mozilla-comm/ical.js)",
  "keywords": [
    "angular",
    "wrapper",
    "calendar",
    "iCalendar",
    "jCal",
    "vCard",
    "jCard",
    "parser"
  ],
  "license": "MIT",
  "homepage": "https://github.com/stevemao/angular-ical",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "app/bower_components",
    "test",
    "tests"
  ],
  "overrides": {
    "ical": {
      "main": "build/ical.js"
    }
  }
}
